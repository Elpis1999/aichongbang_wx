/* components/counter/counter.wxss */
.btns {
    width: 240rpx;
    height: 62rpx;
    display: flex;
}

.btns button {
    width: 33.33%;
    text-align: center;
    line-height: 62rpx;
}

.btns text {
    width: 33.33%;
    box-sizing: border-box;
    border-bottom: 1rpx solid rgb(223, 223, 223);
    border-top: 1rpx solid rgb(223, 223, 223);
    text-align: center;
    line-height: 62rpx;
}