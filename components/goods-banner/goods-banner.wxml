<swiper bind:tap="tap" class="banner" autoplay="true" interval="5000" duration="1000" indicator-dots="true" indicator-color="rgb(255,255,255)" indicator-active-color="#FF4500">
    <block wx:for="{{imgUrls}}" wx:key="{{item}}">
        <swiper-item>
            <image src="{{item}}" class="slide-image" />
        </swiper-item>
    </block>
</swiper>