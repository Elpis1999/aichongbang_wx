/* pages/addresslist/addresslist.wxss */

page{
    background: rgb(233, 233, 233);
}

.address-item {
    display: flex;
    padding: 30rpx 40rpx 30rpx 20rpx;
    justify-content: space-between;
    background-color: white;
    border-bottom: 1rpx solid rgb(233, 233, 233);
}

.btn {
    width: 100%;
    height: 100rpx;
    background-color: orange;
    color: white;
    text-align: center;
    line-height: 100rpx;
    position: fixed;
    bottom: 0;
}